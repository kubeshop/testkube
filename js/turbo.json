{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "ts-check": {
      "dependsOn": ["^ts-check"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
